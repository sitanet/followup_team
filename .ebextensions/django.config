option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "followup_city.settings"
    "PYTHONPATH": "/opt/python/current/app/followup_city:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: followup_city/wsgi.py
    NumProcesses: 3
    NumThreads: 20
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"